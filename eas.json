{
  "cli": {
    "version": ">= 5.2.0",
    "appVersionSource": "remote"
  },
  "build": {
    "development": {
      "developmentClient": true,
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "development",
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "channel": "development"
    },
    "preview": {
      "distribution": "internal",
      "android": {
        "buildType": "apk"
      },
      "env": {
        "EXPO_PUBLIC_APP_ENV": "staging",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "channel": "preview"
    },
    "production": {
      "distribution": "store",
      "android": {
        "buildType": "apk"
      },
      "ios": {
        "buildConfiguration": "Release"
      },
      "autoIncrement": true,
      "env": {
        "EXPO_PUBLIC_APP_ENV": "production",
        "YARN_ENABLE_IMMUTABLE_INSTALLS": "false",
        "EAS_NO_FROZEN_LOCKFILE": "1"
      },
      "channel": "production"
    }
  },
  "submit": {
    "production": {}
  }
}
